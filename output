######### CONFIGURATION 

ddns-update-style none;
default-lease-time 172800;
option domain-name-servers 8.8.4.4,4.4.2.2;
max-lease-time 172850;
authoritative;
log-facility local7;
include "/etc/dhcp/mac.conf";

subnet 185.255.172.0 netmask 255.255.252.0 {
    range 185.255.172.1 185.255.175.255;
    option broadcast-address 185.255.175.255;
    option routers 185.255.172.1;
    option domain-name-servers 8.8.4.4, 4.4.2.2;
    option domain-name "";
}

######### CONFIGURATION 
